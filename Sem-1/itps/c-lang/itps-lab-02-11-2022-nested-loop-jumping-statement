// Online C compiler to run C program online
#include <stdio.h>

int main() {

    int a,b,c,d,e,i;
    
    printf("enter a number: ");
    scanf("%d", &a);
    
    for(i=1; i<=a; i++)
    {
        if(i%2!=0)
            {
                continue;
                
            }
            printf("%d\n", i);
    }
    
    return 0;
}
