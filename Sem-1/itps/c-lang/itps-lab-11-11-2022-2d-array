// program to make 2-D array taking input from user


#include <stdio.h>

int main() {

    int a,i,j;
    int matrix[3][4];
    
    for(i=0; i<3; i++)
        for(j=0; j<4; j++)
        scanf("%d", &matrix[i][j]);
        
    for(i=0; i<3; i++)
    {
        for(j=0; j<4; j++)
        {
        printf("%d\t", matrix[i][j]);
        }
    printf('\n');
    }

    return 0;
}
